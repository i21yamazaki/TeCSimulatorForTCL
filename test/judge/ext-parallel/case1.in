$PRINT-MODE HEX
$RUN
$PARALLEL 03H
$WAIT MS 10
$PRINT PARALLEL     ; C3
$PRINT EXT-PARALLEL ; 03
$PARALLEL 0CH
$WAIT MS 10
$PRINT PARALLEL     ; 3C
$PRINT EXT-PARALLEL ; 0C
$PARALLEL 07H
$WAIT MS 10
$PRINT PARALLEL     ; 87
$PRINT EXT-PARALLEL ; 07
$PARALLEL 0EH
$WAIT MS 10
$PRINT PARALLEL     ; 1E
$PRINT EXT-PARALLEL ; 0E
$STOP
